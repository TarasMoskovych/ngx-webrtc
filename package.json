{
  "name": "ngx-webrtc",
  "version": "19.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:with-href": "ng build --base-href /ngx-webrtc/",
    "build:lib": "ng build --project ngx-webrtc-lib",
    "watch": "ng build --watch --configuration development",
    "publish": "cd ./dist/ngx-webrtc-lib && npm publish",
    "test": "ng test",
    "test:lib": "ng test --project ngx-webrtc-lib",
    "test:ci": "ng test --configuration ci",
    "lint": "ng lint",
    "postinstall": "npm ci --prefix ./projects/ngx-webrtc-lib",
    "serve:ssr:ngx-webrtc": "node dist/ngx-webrtc/server/server.mjs"
  },
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/cdk": "^19.0.5",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/material": "^19.0.5",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/platform-server": "^19.0.5",
    "@angular/router": "^19.0.5",
    "@angular/ssr": "^19.0.6",
    "express": "^4.18.2",
    "ngx-webrtc-lib": "latest",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "browser-sync": "^3.0.0",
    "eslint": "^8.57.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "typescript": "~5.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
