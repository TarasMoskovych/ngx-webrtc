{
  "name": "ngx-webrtc",
  "version": "15.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build --project ngx-webrtc-lib",
    "watch": "ng build --watch --configuration development",
    "publish": "cd ./dist/ngx-webrtc-lib && npm publish",
